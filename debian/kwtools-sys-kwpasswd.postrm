#! /bin/sh
# postrm script for kwtools-sys-kwpasswd-0.0.1
#
# see: dh_installdeb(1)

set -e

if [ -x "`which update-menus 2>/dev/null`" ]; then update-menus ; fi

if [ "$1" = "purge" ] && test -r /usr/share/debconf/confmodule ; then
	. /usr/share/debconf/confmodule
	db_purge
fi

#DEBHELPER#

exit 0
