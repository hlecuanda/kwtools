# Funktion keine VG ausgewählt# {{{
#
# usage: no_select_vg_msg
no_select_vg_msg() {
	MSG=$(gettext 'Sie müssen erst eine VG auswählen.')
	msgbox "$gv_Attention" "$MSG"
}
## }}}
