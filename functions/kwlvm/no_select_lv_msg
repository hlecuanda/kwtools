# Funktion kein LV ausgewaehlt# {{{
#
# usage: no_select_lv_msg
no_select_lv_msg() {
	MSG=$(gettext 'Sie muessen erst ein LV auswaehlen.')
	msgbox "$gv_Attention" "$MSG"
}
## }}}
