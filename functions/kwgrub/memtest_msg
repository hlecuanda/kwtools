# Funktion memtest_msg# {{{
#
# usage: memtest_msg
memtest_msg() {
	unset -- ROOT_PART APPEND INITRD VMLINUZ MAKEACTIVE MODULE MODULE1
	MSG=$(gettext 'Es wurde $MEMTEST ausgewaehlt. Dafuer brauchen Sie nicht die Menuepunkte Kernel, Root_Partition, Append, Makeactive, Initrd, Module und Module1 belegen.')
	infobox "$gv_Info" "$MSG" 3
}
# }}}
