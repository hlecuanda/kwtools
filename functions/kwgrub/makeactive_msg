# Funktion makeactive_msg# {{{
#
# usage: makeactive_msg
makeactive_msg() {
	unset -- ROOT_PART APPEND gv_Initrd gv_Vmlinuz MODULE MODULE1
	MSG=$(gettext 'Es wurde Makeactive ausgewaehlt. Dafuer brauchen Sie nicht die Menuepunkte Root_Partition, Append, Kernel, Initrd, Module und Module1 belegen.')
	infobox "$gv_Info" "$MSG" 3
}
# }}}
