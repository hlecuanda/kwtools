# Funktion, Anzeige das der Wert default geändert werden muss
#
# usage: kwgrub_default_notiz
#
kwgrub_default_notiz() {
	MSG=$(gettext 'Achten Sie auf den richtigen Wert fuer default, in der Konfiguration. Egal ob Sie gerade eine Boot Konfiguration loeschen, veraendern oder anlegen, der Wert aendert sich in jeden Fall. Lesen Sie dazu die Hilfe in der Konfiguration.')
	infobox "$gv_Info" "$MSG" 3
}
#
