# Funktion no_key_serv_msg{{{
#
# usage: no_key_serv_msg
no_key_serv_msg() {
	MSG=$(gettext 'Sie müssen erst einen Schlüsselserver einstellen.')
	msgbox "$gv_Attention" "$MSG"
}
#}}}
