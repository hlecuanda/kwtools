# Funktion, es sind keine RAID ARRAYS vorhanden# {{{
#
# usage: no_raid_arrays_msg
no_raid_arrays_msg() {
	MSG=$(gettext 'Es sind keine RAID Arrays vorhanden.')
	msgbox "$gv_Attention" "$MSG"
}
# }}}
